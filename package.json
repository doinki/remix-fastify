{
  "name": "remix-fastify",
  "version": "0.0.1",
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/remix-fastify/issues",
  "description": "Fastify server request handler for Remix",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/doinki/remix-fastify/tree/main",
  "keywords": [
    "remix",
    "fastify"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doinki/remix-fastify.git"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "@remix-run/node": "^2.0.0",
    "fastify": "^4.0.0"
  },
  "devDependencies": {
    "@remix-run/node": "^2.4.1",
    "@types/node": "^20.10.6",
    "esbuild": "^0.19.11",
    "fastify": "^4.25.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
